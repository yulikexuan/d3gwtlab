<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HTMLPanel>

		<g:TabLayoutPanel barUnit='EM' barHeight='3' width="100%" height="100%">
			<g:tab>
				<g:customHeader size='7'>
					<g:Label>Polar Chart</g:Label>
				</g:customHeader>
				<g:SimpleLayoutPanel ui:field="polarChartPanel"></g:SimpleLayoutPanel>
			</g:tab>
			<g:tab>
				<g:customHeader size='7'>
					<g:Label>Symbols</g:Label>
				</g:customHeader>
				<g:SimpleLayoutPanel ui:field="symbolsPanel"></g:SimpleLayoutPanel>
			</g:tab>
		</g:TabLayoutPanel>

	</g:HTMLPanel>

</ui:UiBinder>